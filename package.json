{
  "name": "smoking-cessation-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "ai:dev": "node server/index.js",
    "dev:client": "vite --host",
    "dev:server": "npx nodemon --watch server --ext js,mjs,cjs server/index.js",
    "dev:all": "concurrently -n server,client -c magenta,blue \"npm:dev:server\" \"npm:dev:client\"",
    "build:mobile": "vite build --mode mobile",
    "android:sync": "npx cap copy android && npx cap sync android",
    "android:run": "cd android && gradlew.bat assembleDebug installDebug",
    "android:build": "cd android && gradlew.bat assembleDebug",
    "adb:reverse": "\"%LOCALAPPDATA%\\Android\\Sdk\\platform-tools\\adb.exe\" reverse tcp:8787 tcp:8787",
    "adb:devices": "\"%LOCALAPPDATA%\\Android\\Sdk\\platform-tools\\adb.exe\" devices",
    "dev:mobile": "npm-run-all -p dev:all adb:reverse",
    "run:android": "npm run build:mobile && npx cap sync android && npx cap run android",
    "seed:challenges": "node seed_challenges.cjs"
  },
  "dependencies": {
    "@capacitor/android": "7.4.2",
    "@capacitor/app": "7.0.2",
